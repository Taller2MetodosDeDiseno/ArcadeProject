        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Controles de Daño

// Control de disponibilidad

if (place_meeting(x+94,y,objEnem))||
    (place_meeting(x-94,y,objEnem)){
canatq = true;
}
else{
canatq = false;
}

// Comandos de ataque

atbas = keyboard_check_pressed(vk_space);
atdist = keyboard_check_pressed(vk_shift);
ultiatq = keyboard_check_pressed(vk_enter);

// Ataques

if (atbas)&amp;&amp;
    (canatq){
global.atbas = true;
sprmelee = true;
}
else{
global.atbas = false
}

// Control de Daño Recibido

if (place_meeting(x,y,objBalaBullyBas)){
bulletwho = 1
}

if (place_meeting(x,y,objBalaBullyMed)){
bulletwho = 1.1
}

if (place_meeting(x,y,objBalaHistBad)){
bulletwho = 2
}

if (place_meeting(x,y,objBalaHistGood)){
bulletwho = 2.1
}

if (place_meeting(x,y,objBalaNerdBas)){
bulletwho = 3
}

if (place_meeting(x,y,objBalaNerdMed)){
bulletwho = 3.1
}

if (place_meeting(x,y,objBalaQuimBad)){
bulletwho = 4
}

if (place_meeting(x,y,objBalaQuimGood)){
bulletwho = 4.1
}

if (place_meeting(x,y,objBalaDeportBas)){
bulletwho = 5
}

if (place_meeting(x,y,objBalaDeportMed)){
bulletwho = 5.1
}

if (place_meeting(x,y,objBalaEdFBad)){
bulletwho = 6
}

if (place_meeting(x,y,objBalaEdFGood)){
bulletwho = 6.1
}


if (place_meeting(x,y,objBalaEnem)){
switch (bulletwho){
    case 1: hlth = hlth - objBullyBas.dmg; break;
    case 1.1: hlth = hlth - objBullyMed.dmg; break;
    case 2: hlth = hlth - objProfHist.dmg; break;
    case 3: hlth = hlth - objNerdBas.dmg; break;
    case 3.1: hlth = hlth - objNerdMed.dmg; break;
    case 4: hlth = hlth - objProfQuim.dmg; break;
    case 5: hlth = hlth - objDeportBas.dmg; break;
    case 5.1: hlth = hlth - objDeportMed.dmg; break;
    case 6: hlth = hlth - objProfEdF.dmg; break;
    }
}

if (place_meeting(x,y,objBalaEnemGood))&amp;&amp;(keyboard_check(vk_space)){
switch (bulletwho){
    case 2.1: objBalaHistGood.hspeed = -(objBalaHistGood.hspeed); break;
    case 4.1: objBalaQuimGood.hspeed = -(objBalaQuimGood.hspeed); break;
    case 6.1: objBalaEdFGood.hspeed = -(objBalaEdFGood.hspeed); break;
    }
}

if (hlth &lt;= 0){
if(canend){
alarm[6] = 35;
    }
canend = false;
if (theend){
    show_message("Te has quedado sin salud");
    room_restart();
    }
}


