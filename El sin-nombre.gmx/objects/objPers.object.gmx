}


// Power-Ups de Vida.

if(place_meeting(x,y,objPapitas)){
hlth = hlthmax;
}


// Cronómetro Power_Up Velocidad.

if (xvel = 11){
powervel = powervel - 1;
}


// Cronómetros Power_Up Ataque.

if (xtradmg = 3){
powerdmg = powerdmg - 1;
}

if (xtradmg = 5){
powerdmg = powerdmg - 1;
}

if (xtradmg = 7){
powerdmg = powerdmg - 1;
}


// Cronómetro Power_Up Defensa.

if (hlthmax = 110){
powerhlth = powerhlth - 1;
}

if (hlthmax = 120){
powerhlth = powerhlth - 1;
}

if (hlthmax = 130){
powerhlth = powerhlth - 1;
}

