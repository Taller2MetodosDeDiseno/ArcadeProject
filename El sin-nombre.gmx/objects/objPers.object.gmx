powerup = 1
}

if (place_meeting(x,y,objShieldMed)){
hlthmax = 120;
    if(hlth &lt; hlthmax - 20){
    hlth = hlth + 20;
    }
    else{
    hlth = hlthmax;
    }
alarm[2] = 450;
powerup = 2
}

if (place_meeting(x,y,objShieldTop)){
hlthmax = 130;
    if(hlth &lt; hlthmax - 30){
    hlth = hlth + 30;
    }
    else{
    hlth = hlthmax;
    }
alarm[2] = 450;
powerup = 3
}


// Power-Ups de Velocidad.

if (place_meeting(x,y,objEnergiz)){
xvel = 11;
