}

if (place_meeting(x,y,objArmaTop)){
xtradmg = 7;
alarm[1] = 450;
powerup = 3
}


// Power-Ups de Defensa.

if (place_meeting(x,y,objShieldBas)){
hlthmax = 110;
    if(hlth &lt; hlthmax - 10){
    hlth = hlth + 10;
    }
    else{
    hlth = hlthmax;
    }
