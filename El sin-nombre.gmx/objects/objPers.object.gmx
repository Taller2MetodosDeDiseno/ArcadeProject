            <string>/// Cambios de Sprites

if (sprcorrerD)||(sprcorrerI){
sprcorrer = true;
switch (powerup){
    case 0:
    case 1: sprite_index = sprPersCorrerBas; break;
    case 2: sprite_index = sprPersCorrerMed; break;
    case 3: sprite_index = sprPersCorrerTop; break;
    }
}
else{
sprcorrer = false;
}

if ((!sprcorrer)&amp;&amp;(!sprmelee)&amp;&amp;(!sprmuere)&amp;&amp;(!sprdistancia)&amp;&amp;(!sprdefensa)){
switch (powerup){
    case 0:
    case 1: sprite_index = sprPersQuietoBas; break;
    case 2: sprite_index = sprPersQuietoMed; break;
    case 3: sprite_index = sprPersQuietoTop; break;
    }
}

if (global.atbas){
alarm[3] = 15;
switch (powerup){
    case 0:
    case 1: sprite_index = sprPersMeleeBas; break;
    case 2: sprite_index = sprPersMeleeMed; break;
    case 3: sprite_index = sprPersMeleeTop; break;
    }
}

if (hlth &lt;= 0){
sprmuere = true;
switch (powerup){
    case 0:
    case 1: sprite_index = sprPersMuereBas; break;
    case 2: sprite_index = sprPersMuereMed; break;
    case 3: sprite_index = sprPersMuereTop; break;
    }
}

if (atdist){
alarm[4] = 9;
sprite_index = sprPersDistancia
}

if (place_meeting(x + 40,y,objBalaEnem))||
    (place_meeting(x - 40,y,objBalaEnem)){
sprdefensa = true;
alarm[5] = 12;
switch (powerup){
    case 0:
    case 1: sprite_index = sprPersDefensaBas; break;
    case 2: sprite_index = sprPersDefensaMed; break;
    case 3: sprite_index = sprPersDefensaTop; break;
    }
}
