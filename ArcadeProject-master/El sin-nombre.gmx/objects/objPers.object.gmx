            <string>/// Movimiento del Personaje

// Resumen de Comandos

up = (keyboard_check_pressed(vk_up));
down = (keyboard_check_pressed(vk_down));
left = (keyboard_check(vk_left));
right = (keyboard_check(vk_right));


// Controles de Disponibilidad

upcan = (y - yvel &gt;= 192);
downcan = (y + yvel &lt;= 672);
leftcan = (x - xvel &gt; 48);
rightcan = (x + xvel &lt; 1296);


// Comandos de Movimiento

if (up)&amp;&amp;(upcan){
y = y - yvel;
}

if (down)&amp;&amp;(downcan){
y = y + yvel;
}

if (right)&amp;&amp;(rightcan){
image_xscale = 1;
sprite_index = sprPersD;
    if(!place_meeting(x + xvel,y,objEnem)){
    x = x + xvel
    }
}

if (left)&amp;&amp;(leftcan){
image_xscale = -1;
sprite_index = sprPersI;
    if(!place_meeting(x - xvel,y,objEnem)){
    x = x - xvel
    }
}

global.dir = image_xscale;
</string>
